{
  "name": "instalog",
  "displayName": "InstaLog",
  "description": "Just log a variable with a simple keybind",
  "version": "1.0.0",
  "icon": "icon.png",
  "publisher": "ynkf",
  "repository": {
    "type": "git",
    "url": "https://github.com/ynkf/InstaLog"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.108.1"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "instalog.logAfterSelection",
        "title": "Log After Selection"
      },
      {
        "command": "instalog.logBeforeSelection",
        "title": "Log Before Selection"
      }
    ],
    "keybindings": [
      {
        "command": "instalog.logAfterSelection",
        "key": "alt+-",
        "mac": "alt+-",
        "when": "editorTextFocus"
      },
      {
        "command": "instalog.logBeforeSelection",
        "key": "alt+.",
        "mac": "alt+.",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run build",
    "build": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "vsce package",
    "publish": "vsce publish",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test",
    "test:unit": "npm run compile-tests && mocha --ui tdd out/test/**/*.spec.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "24.x",
    "@types/vscode": "^1.108.1",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "@vscode/vsce": "^3.7.1",
    "esbuild": "^0.27.2",
    "eslint": "^9.39.2",
    "npm-run-all2": "^8.0.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0"
  }
}
